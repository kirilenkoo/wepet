/**app.wxss**/
page {
  font-size: 28rpx;
  color: #2a2a2a;
  background: white;
}
page::after{
  visibility: visible;
  width: 0;
  height: 0;
  display: block;
  clear: both;
  content: '';
}
.container {
  display: flex;
  flex-direction: column;
  flex-wrap:wrap;
  align-items: left;
  box-sizing: border-box;
} 